[cxx]
  default_platform = iphonesimulator-x86_64
  cflags = -g -fobjc-arc 

[apple]
  default_debug_info_format_for_binaries = NONE
  default_debug_info_format_for_libraries = NONE
  xctool_default_destination_specifier = platform=iOS Simulator,OS=latest
  xctool_path = xctool/bin/xctool

[project]
    ide = xcode
    ignore = .buckd, \
             .git, \
             buck-out, \
